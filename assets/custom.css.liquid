         url("{{ 'BrownRegular.woff' | asset_url }}") format("woff"), 
         url("{{ 'BrownStd-Regular.otf' | asset_url }}") format("opentype");
}
  @font-face {
    font-family: "BrownStd";
    font-style: italic;
    font-weight: 400;
    font-display: swap;
