         url("{{ 'BrownRegular.woff' | asset_url }}") format("woff"), 
         url("{{ 'BrownStd-Regular.otf' | asset_url }}") format("opentype");
}
  @font-face {
    font-family: "BrownStd";
    font-style: normal;
    font-weight: 700;
    font-display: swap;
    src: url("{{ 'BrownBold.woff2' | asset_url }}") format("woff2"), 
         url("{{ 'BrownBold.woff' | asset_url }}") format("woff"), 
         url("{{ 'BrownStd-Bold.otf' | asset_url }}") format("opentype");
}
  @font-face {
    font-family: "BrownStd";
    font-style: italic;
    font-weight: 700;
    font-display: swap;
    src: url("{{ 'BrownBold.woff2' | asset_url }}") format("woff2"), 
