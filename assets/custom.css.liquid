}
.popup-close{
    position: absolute;
    right: 0;
    top: 0;
    padding: 2px;
    width: 30px;
    height: 30px;
    background-color: transparent;
    background-clip: content-box;
    cursor: pointer;
    opacity: 1;
    -webkit-appearance: none;
    border-radius: 100%;
    border: solid 2px transparent;
    box-sizing: content-box;
    transition: all 0.2s ease-in-out;
}
.popup-close:before,
.popup-close:after{
    display: block;
    content: " ";
    position: absolute;
    top: 7px;
    left: 15px;
    height: 20px;
    border-radius: 4px;    
    width: 1px;
    transition: all 0.2s ease;
    opacity: 0.6;
    background-color: #000;
}
.popup-close:before {
    transform: rotate(-45deg);
}
.popup-close:after {
    transform: rotate(45deg);
}

.popup-close:hover:before,
.popup-close:hover:after{
    opacity: 1;
}
