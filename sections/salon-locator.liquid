        <div class="location-box">
            {% if section.blocks.size > 0 %}
                <div class="location-tabs tab-links">
                    {% for block in section.blocks %}
                        {% assign tabPage = pages[block.settings.location] %}
                        <a href="{{ tabPage.url }}" class="tab-link {% if tabPage.handle == page.handle %}active{% endif %}">
                            {{ block.settings.title }}
                        </a>
                    {% endfor %}
                </div>
            {% endif %}

            <div class="location-search">
                {% if section.settings.description != blank %}
                    <div class="location-description">
                        {{ section.settings.description }}
                    </div>
                {% endif %}
                <div class="location-form">
                    <form method="post" id="location-form">
                        <div class="input-field">
                            <label>&nbsp;</label>
                            <input type="text" class="field__input" placeholder="enter suburb or postcode" name="location-keyword">
                        </div>
                        {% assign regions = regionList | split:',' | uniq | sort %}
                        <div class="select-field">
                            <label>state/region</label>
                            <select class="select__select region-select" name="location-region">
                                <option value="all" selected>all</option>
                                {% for region in regions %}
                                    {% unless forloop.first %}
                                        <option value="{{region}}">{{region}}</option>
                                    {% endunless %}
                                {% endfor %}
                            </select>
                        </div>
                        <div class="input-button">
                            <button type="submit" class="button button--secondary">
                                search
                            </button>
                        </div>
                    </form>
                </div>

                <div class="result-summary">
                    <span class="result-current">1</span>
                    -
                    <span class="result-pagination">
                        {{ locationCount }}
                    </span>
                    of
                    <span class="result-total">{{ locationCount }}</span>
                    {{ locationCount | pluralize: 'item', 'items' }}
                </div>
            </div>

        </div>
        <div id="location-list">
            {{ location_list }}
            <div class="not-found hidden">No locations found.</div>
        </div>
        {% comment %}
            <div class="location-pagination">
                <span class="pagination-status">
                    page
                    <span class="current">1</span>
                    of
                    <span class="total">{{ pageNumber }}</span>
                </span>
                <div class="pagination-list" data-current-page="1">
