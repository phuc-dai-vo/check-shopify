<div class="location-list-section">
    <div class="page-width">
        {% assign regionList = '' %}
        {% assign locationCount = 0 %}
        {% capture location_list %}
            {% assign sections = page.content | split: '***' %}
            {% for section in sections %}
                {% assign parts = section | strip_html | split: '---' %}
                {% assign title_state = parts[0] | split:'-' %}
                {% assign title = title_state[0] | strip_newlines | strip | strip_html %}
                {% assign state = title_state[1] | handleize %}
                {% assign regionList = regionList | append:',' | append:state %}
                {% if title != blank %}
                    {% assign pageNumber = locationCount | plus:1 | divided_by:2.0 | ceil %}
                    {% assign firstPageItems = false %}
                    {% if pageNumber == 2 %}
                        {% assign firstPageItems = locationCount %}
                    {% endif %}
                    <div class="location-item active" data-region="{{ state }}" data-page="{{ pageNumber }}">
                        <h3>
                            <span>{{ title }}</span>
                            <svg viewBox="0 0 8 8" fill="none" id="small-chevron-down">
                                <path fill-rule="evenodd" clip-rule="evenodd" d="M0.5 2.74749L1.48995 1.75754L3.99497 4.26256L6.5 1.75754L7.48995 2.74749L3.99497 6.24246L0.5 2.74749Z" fill="currentColor"></path>
                                </svg>
                        </h3>
                        <div class="location-content">
                            {% for part in parts %}
                                {% if forloop.index > 1 and part != '' %}
                                    {% assign block_parts = part | split: 'address:' %}
                                    {% assign block_title = block_parts[0] | replace: '[', '' | replace: ']', '' %}
                                    {% assign address_parts = block_parts[1] | split: 'enquiries:' %}
                                    {% assign block_address = address_parts[0] | newline_to_br | strip %}
                                    {% assign block_enquiries = address_parts[1] | newline_to_br | strip %}
                                    {% assign locationCount = locationCount | plus:1 %}
                                    <div class="location-block">
                                    <h4>
